class Solution {
    public boolean searchMatrix(int[][] matrix, int target) {
        int n=mat.length;
        int  m=mat[0].length;
        int low=0;
        int high=n*m-1;
        while(low<=high){
            int mid=(low+high)/2;
            int row=mid/2;
            int col=mid%2;
            if(mat[row][col]==target)return true;
            else if(mat[row][col]<target)return
        }
    }
}
